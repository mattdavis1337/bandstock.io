<%= form_for @changeset, sheet_path(@conn, :create), fn f -> %>
	<div class="form-group">
		<%= text_input f, :name, placeholder: "Sheet Name", class: "form-control" %>
		<%= text_input f, :hash, placeholder: "Hash", class: "form-control" %>

		<%= submit "Create Sheet", class: "btn btn-primary" %>
<% end %>